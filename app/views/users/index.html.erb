<%= render 'books/list', user: @user, book: @book %>

<!--右側のUsers部分-->
<h2>Users</h2>
<table>
  <tr>
    <th>image</th>
    <th>mane</th>
    <th></th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td>
      <%= image_tag user.get_profile_image %>
    </td>
    <td>
      <%= user.name %>
    </td>
    <td>
      <%= link_to "show", user_path(user.id) %>
    </td>
  </tr>
  <% end %>

</table>
